{
  "compilerOptions": {
      "target": "es6",
      "allowJs": true,
      "jsx": "react",
      "outDir": "./dist",
      "sourceMap": false,
      "noImplicitAny": false,
      "module": "commonjs",
      "watch": true,
      "types": [
          "react","react-native"
      ],
      "skipLibCheck": true,
      "moduleResolution":"node"
  },
  "include": [
      "typings/**/*.d.ts",
      "src/**/*.ts",
      "src/**/*.tsx",
      "*.tsx"
  ],
  "exclude": [
      "node_modules"
  ], 
}